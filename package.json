{
  "name": "eol",
  "description": "Newline character converter",
  "version": "0.3.0-0",
  "homepage": "https://github.com/ryanve/eol",
  "license": "MIT",
  "author": "Ryan Van Etten",
  "keywords": [
    "eol",
    "newline",
    "convert",
    "converter",
    "conversion",
    "character",
    "ender"
  ],
  "main": "eol.js",
  "scripts": {
    "lint": "jshint eol.js && jshint test.js",
    "test": "npm run lint && node test.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ryanve/eol.git"
  },
  "bugs": {
    "url": "https://github.com/ryanve/eol/issues"
  },
  "devDependencies": {
    "jshint": "^2.9.3",
    "aok": "^1.9.0"
  },
  "jshintConfig": {
    "asi": true,
    "boss": true,
    "browser": true,
    "eqnull": true,
    "expr": true,
    "latedef": "nofunc",
    "laxcomma": true,
    "maxerr": 5,
    "node": true,
    "sub": true,
    "supernew": true,
    "undef": true,
    "unused": true
  }
}
